<mapper namespace="com.ztesoft.core.db.dao.mapper.ISequenceFuncMapper">
	<!-- 调用序列生成函数，生成 序列号  -->
	<select id="selectSequence" resultType="String" parameterType="map">
	<![CDATA[
		   SELECT SequenceFunc(#{tableName},#{columnName},#{schemaName},#{count})
	]]>
	</select>
    
</mapper>